{
  "name": "@mochi-inc-japan/algolia-firebase-tools",
  "version": "1.10.1",
  "main": "dist/index.js",
  "bin": {
    "aftools": "dist/bin/aftools.js",
    "aftools-build": "dist/bin/aftools-build.js"
  },
  "types": "dist/index.d.ts",
  "repository": "git@github.com:MOCHI-inc-JAPAN/algolia-firebase-tools.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "tkow <t.kusama0613@outlook.jp>",
  "license": "MIT",
  "files": [
    "template",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npx tsc",
    "test": "jest",
    "test:inspect": "node --inspect-brk node_modules/.bin/jest --watch --no-cache --runInBand",
    "lint": "yarn lint:code",
    "lint:ci": "yarn lint:code && commitlint --from=origin/develop",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:commit": "commitlint",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/algoliasearch": "^3.34.10",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-jest": "^26.3.0",
    "commander": "^5.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-admin": "^9.0.0",
    "firebase-tools": "^8.7.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.2.0"
  },
  "dependencies": {
    "algoliasearch": "^4.3.0",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "typescript": ">=3.9.6"
  },
  "peerDependencies": {
    "algoliasearch": "^4.3.0",
    "firebase-admin": "^9.0.0",
    "typescript": ">=3.9.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint",
      "pre-push": "npm test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
